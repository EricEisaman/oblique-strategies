{
  "name": "oblique-strategies-client",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "cleanup": "pkill -f 'vite' || true",
    "dev": "npm run cleanup && vite",
    "build": "vue-tsc --noEmit -p tsconfig.typecheck.json && vite build --mode production",
    "build:vite": "vite build --mode production",
    "build:type-check": "vue-tsc && vite build",
    "preview": "vite preview",
    "type": "vue-tsc --noEmit -p tsconfig.typecheck.json",
    "type-check": "vue-tsc --noEmit",
    "t": "vue-tsc --noEmit -p tsconfig.typecheck.json && cd ../server && npx tsc --noEmit && cd ../client",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore"
  },
  "dependencies": {
    "@mdi/font": "^7.3.67",
    "axios": "^1.6.0",
    "pinia": "^2.1.7",
    "vue": "^3.5.19",
    "vue-router": "^4.2.5",
    "vuetify": "^3.9.6"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^26.1.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  }
}
